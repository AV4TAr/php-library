  "require": {
    "php": ">=7.1",
    "wework/error": "^1.1",
    "wework/we-http-client": "^2",
    "php-amqplib/php-amqplib": "^2.6",
    "psr/log": "1.0.x-dev",
    "wework/platform-common": "^1.5",
    "elasticsearch/elasticsearch": "~5.0",
    "monolog/monolog": "~1.0"
