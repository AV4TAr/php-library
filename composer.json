    "phpunit/php-timer": "^2",
    "phpunit/php-token-stream": "^3",
    "phpunit/phpunit-mock-objects": "^6",
    "sebastian/comparator": "^2",
    "sebastian/diff": "^3",
    "sebastian/environment": "^3.1",
